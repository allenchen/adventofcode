import math

def run(instructions):
    lines = instructions.splitlines()
    buses = lines[0].split(",")

    track = []
    bus_index = 0
    timestamp = 1
    
    while True:
        bus = buses[bus_index]
        if bus == "x" or timestamp % int(bus) == 0:
            track += [bus]
            bus_index += 1
            if bus_index == len(buses):
                print "Done!"
                print timestamp
                break
        else:
            bus_index = 0
            track = []

        timestamp += 1
        

f = open("13.txt", "r")
result = run(f.read())
print result
print result[0] * result [1]
